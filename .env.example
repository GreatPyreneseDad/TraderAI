# API Keys
CLAUDE_API_KEY=your_claude_api_key_here
TIINGO_API_TOKEN=your_tiingo_token_here

# Database Configuration
DATABASE_URL=postgresql://trader_ai_user:password@localhost:5432/trader_ai
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET=your-ultra-secure-secret-minimum-64-chars-change-this-in-production
CORS_ORIGIN=http://localhost:3000

# Server Configuration
NODE_ENV=development
PORT=3000
FRONTEND_PORT=5173
DASHBOARD_PORT=8080

# ECNE Performance Settings
ECNE_MAX_CONCURRENT=50
ECNE_BATCH_SIZE=100
ECNE_CACHE_MAX_ITEMS=10000
ECNE_CACHE_TTL_SECONDS=300

# GCT Configuration
GCT_COHERENCE_THRESHOLD_PSI=0.7
GCT_COHERENCE_THRESHOLD_RHO=0.6
GCT_COHERENCE_THRESHOLD_Q=0.5
GCT_COHERENCE_THRESHOLD_F=0.4

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000
WS_RECONNECT_ATTEMPTS=5

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Worker Configuration
WORKER_PROCESSES=4
WORKER_MEMORY_LIMIT_MB=512